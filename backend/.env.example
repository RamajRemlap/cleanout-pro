# CleanoutPro Backend Environment Variables
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=postgresql://cleanout:cleanout_dev_password@localhost:5432/cleanoutpro

# Redis
REDIS_URL=redis://localhost:6379

# Ollama AI
OLLAMA_URL=http://localhost:11434

# PayPal Configuration
PAYPAL_MODE=sandbox
# Get these from https://developer.paypal.com
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
PAYPAL_WEBHOOK_ID=your_webhook_id_here

# Application
APP_URL=http://localhost:3000
API_URL=http://localhost:8000

# File Storage
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE_MB=10

# Security (generate with: openssl rand -hex 32)
SECRET_KEY=your_secret_key_here_replace_with_random_string

# Environment
ENVIRONMENT=development
# development, staging, production

# Logging
LOG_LEVEL=INFO
# DEBUG, INFO, WARNING, ERROR, CRITICAL
